{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/personal-finance-management-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/personal-finance-management-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/personal-finance-management-system",
  "projects": {
    "personal-finance-management-system": {
      "type": "application",
      "root": "apps/personal-finance-management-system",
      "entryFile": "main",
      "sourceRoot": "apps/personal-finance-management-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/personal-finance-management-system/tsconfig.app.json"
      }
    },
    "report-service": {
      "type": "application",
      "root": "apps/report-service",
      "entryFile": "main",
      "sourceRoot": "apps/report-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-service/tsconfig.app.json"
      }
    },
    "analytics-service": {
      "type": "application",
      "root": "apps/analytics-service",
      "entryFile": "main",
      "sourceRoot": "apps/analytics-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics-service/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    }
  }
}